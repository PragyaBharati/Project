<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
</head>
<body>
    <header>
        <div style="position:center;top:20px;width:100%;height:200%;left:10px;right:10px;background-color:#52b2bf">
            <div class="container">
                <a class="navbar-brand" style="margin-left:120px;color:white" asp-controller="Home" asp-action="Index">LabourZilla Zone</a>
                <ol class="navbar-brand" style="text-decoration:none">

                    <li class="navbar-brand" style="margin-left:20px">
                        <a class="navbar-brand" style="color:#000000" asp-area="" asp-controller="Labours" asp-action="Index"><b>Labours</b></a>
                        <a class="navbar-brand" style="color:#000000" asp-area="" asp-controller="Labours" asp-action="Index"><b>Login</b></a>
                        <a class="navbar-brand" style="color:#000000" asp-area="" asp-controller="Labours" asp-action="Index"><b>SignUp</b></a>
                        <a class="navbar-brand" style="color:#000000" asp-area="" asp-controller="Labours" asp-action="Index"><b>About Us</b></a>
                        
                    </li>
                </ol>
            </div>
        </div>
        
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
